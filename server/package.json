{
  "name": "mc-api",
  "version": "1.0.0",
  "description": "Mooncascade time tracking API",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "docker:dev-db": "docker-compose up -d && npm run db:reset"
  },
  "author": "Patryk Kud≈Ça <patt.kudla@gmail.com> (https://github.com/Lotafak)",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "pg": "^7.7.1",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "socket.io": "^2.2.0",
    "swagger-ui-express": "^4.0.2"
  }
}
